<!--pages/message/message.wxml-->
<view class='space'></view>
<view wx:for="{{list}}"  wx:key="{{index}}">
  <view class='normal' data-type="{{item.type}}" bindtap='skipToPage'>
    <view class='type'>【{{item.tag}}】{{item.title}}</view>
    <view class='status'>{{item.msg}}</view>
    <view class='small'>{{item.sentTime}}</view>
  </view>
</view>
